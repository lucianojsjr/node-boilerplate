{
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "parserOptions": {
    "sourceType": "module",
    "ecmaFeatures": {
      "arrowFunctions": true,
      "blockBindings": true,
      "classes": true,
      "defaultParameters": true,
      "destructuring": true,
      "forOf": true,
      "generators": true,
      "modules": true,
      "objectLiteralComputedProperties": true,
      "objectLiteralDuplicateProperties": true,
      "objectLiteralShorthandMethods": true,
      "objectLiteralShorthandProperties": true,
      "regexUFlag": true,
      "regexYFlag": true,
      "restParams": true,
      "spread": true,
      "superInFunctions": true,
      "templateStrings": true,
      "unicodeCodePointEscapes": true,
      "globalReturn": true
    }
  },
  "rules": {
    "curly": 2,
    "eqeqeq": 2,
    "no-magic-numbers": 0,
    "block-scoped-var": 2,
    "strict": [
      2,
      "never"
    ],
    "keyword-spacing": [
      2,
      {
        "before": true,
        "after": true
      }
    ],
    "no-multiple-empty-lines": [
      2,
      {
        "max": 2
      }
    ],
    "yoda": 2,
    "wrap-iife": 2,
    "new-parens": 2,
    "vars-on-top": 2,
    "no-new-require": 2,
    "no-process-exit": 2,
    "no-process-env": 0,
    "no-with": 2,
    "no-unused-expressions": 2,
    "no-useless-escape": 2,
    "no-useless-concat": 2,
    "no-floating-decimal": 2,
    "no-implied-eval": 2,
    "no-new": 2,
    "no-new-func": 2,
    "no-octal": 2,
    "no-new-wrappers": 2,
    "no-redeclare": 2,
    "no-return-assign": 2,
    "no-lone-blocks": 2,
    "no-loop-func": 2,
    "no-multi-spaces": 2,
    "no-multi-str": 2,
    "no-native-reassign": 2,
    "no-empty-pattern": 2,
    "no-case-declarations": 2,
    "dot-notation": 2,
    "no-cond-assign": 2,
    "no-dupe-args": 2,
    "no-dupe-keys": 2,
    "no-duplicate-case": 2,
    "no-delete-var": 2,
    "no-label-var": 2,
    "no-unused-vars": 2,
    "no-catch-shadow": 2,
    "no-undef": 2,
    "no-undefined": 2,
    "object-property-newline": 2,
    "semi": [
      2,
      "always"
    ],
    "padded-blocks": [
      2,
      "never"
    ],
    "one-var": [
      2,
      "never"
    ],
    "no-mixed-requires": 2,
    "no-empty": 2,
    "no-ex-assign": 2,
    "no-extra-boolean-cast": 2,
    "no-extra-semi": 2,
    "no-func-assign": 2,
    "no-inner-declarations": 2,
    "no-irregular-whitespace": 2,
    "no-negated-in-lhs": 2,
    "no-obj-calls": 2,
    "use-isnan": 2,
    "valid-typeof": 2,
    "array-callback-return": 2,
    "no-unreachable": 2,
    "no-unexpected-multiline": 2,
    "no-sparse-arrays": 2,
    "no-constant-condition": 2,
    "comma-dangle": [
      2,
      "never"
    ],
    "linebreak-style": [
      2,
      "windows"
    ],
    "no-mixed-spaces-and-tabs": 2,
    "no-trailing-spaces": 2,
    "indent": [
      2,
      "tab",
      {
        "SwitchCase": 1
      }
    ],
    "quotes": [
      2,
      "single"
    ],
    "array-bracket-spacing": [
      2,
      "never"
    ],
    "block-spacing": [
      2,
      "always"
    ],
    "brace-style": [
      2,
      "1tbs",
      {
        "allowSingleLine": false
      }
    ],
    "comma-spacing": [
      2,
      {
        "before": false,
        "after": true
      }
    ],
    "comma-style": [
      2,
      "last"
    ],
    "computed-property-spacing": [
      2,
      "never"
    ],
    "consistent-this": [
      2,
      "that"
    ],
    "no-whitespace-before-property": 2,
    "no-underscore-dangle": 2,
    "no-new-object": 2,
    "no-spaced-func": 2,
    "unicode-bom": [
      2,
      "never"
    ],
    "eol-last": 0,
    "func-style": [
      "error",
      "declaration",
      {
        "allowArrowFunctions": true
      }
    ],
    "newline-after-var": [
      2,
      "always"
    ],
    "no-continue": 2,
    "no-lonely-if": 2,
    "no-mixed-operators": 2,
    "no-bitwise": [
      2,
      {
        "allow": [
          "~"
        ]
      }
    ],
    "no-array-constructor": 2,
    "space-before-function-paren": [
      2,
      "never"
    ],
    "space-in-parens": [
      2,
      "never"
    ]
  }
}